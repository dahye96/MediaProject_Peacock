<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.peac.cock.peacock_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LodingActivity"
            android:label="@string/login_layout_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EmailJoinActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MemberInfoActivitiy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AssetActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CalculatorActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddCashActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddCardActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HandwritingActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SettingActivity"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>